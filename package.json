{
  "name": "nuxt-windicss",
  "version": "0.5.4",
  "description": "Windi CSS for Nuxt.js",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "windicss",
    "tailwindcss"
  ],
  "homepage": "https://github.com/windicss/nuxt-windicss-module",
  "bugs": "https://github.com/windicss/nuxt-windicss-module/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/windicss/nuxt-windicss-module"
  },
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./": "./",
    "./src/template/*": "./dist/template/*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "build:basic": "yarn build && export DEBUG=windicss-webpack-plugin && nuxt build test/fixtures/basic",
    "build:preset": "export NODE_ENV=production && nuxt build test/fixtures/preset",
    "build:static": "export NODE_ENV=production && nuxt build test/fixtures/static",
    "build:vuetify": "export NODE_ENV=development && nuxt build test/fixtures/vuetify",
    "dev:basic": "nuxt test/fixtures/basic",
    "dev:vite": "npm run build && nuxt test/fixtures/vite",
    "dev:bootstrap": "nuxt test/fixtures/bootstrap-tailwind",
    "dev:preset": "nuxt test/fixtures/preset",
    "dev:static": "nuxt test/fixtures/static",
    "dev:vuetify": "yarn build && export DEBUG=windicss-webpack-plugin && nuxt test/fixtures/vuetify",
    "dev:xs": "NODE_ENV=dev nuxt test/fixtures/xs --spa",
    "generate:static": "export NODE_ENV=production && nuxt generate test/fixtures/static",
    "generate:vuetify": "export NODE_ENV=production && nuxt generate test/fixtures/vuetify",
    "lint": "eslint --fix --ext .ts,.js,.vue .",
    "measure": "export NUXT_MEASURE=true && nuxt test/fixtures",
    "release": "dotenv release-it --",
    "start:basic": "export NODE_ENV=production && nuxt start test/fixtures/basic",
    "start:preset": "export NODE_ENV=production && nuxt start test/fixtures/preset",
    "start:static": "export NODE_ENV=production && nuxt start test/fixtures/static",
    "start:vuetify": "export NODE_ENV=production && nuxt start test/fixtures/vuetify",
    "test": "yarn lint && jest --verbose ",
    "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
  },
  "dependencies": {
    "clear-module": "^4.1.1",
    "defu": "^3.2.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "upath": "^2.0.1",
    "vite-plugin-windicss": "^0.12.5",
    "windicss-webpack-plugin": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.2",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@nuxt/content": "^1.14.0",
    "@nuxt/image": "^0.4.1",
    "@nuxt/test-utils": "0.2.0",
    "@nuxt/types": "2.15.3",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/svg": "^0.1.12",
    "@nuxtjs/tailwindcss": "^4.0.2",
    "@nuxtjs/vuetify": "^1.11.3",
    "@tailwindcss/typography": "^0.4.0",
    "@types/jest": "^26.0.22",
    "@types/loader-utils": "2.0.2",
    "@types/lodash": "latest",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.36",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "latest",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "bootstrap-vue": "^2.21.2",
    "cheerio": "^1.0.0-rc.5",
    "dotenv-cli": "^4.0.0",
    "element-ui": "^2.15.1",
    "eslint": "7.22.0",
    "eslint-webpack-plugin": "^2.5.3",
    "jest": "latest",
    "loader-utils": "latest",
    "minify-css-string": "^1.0.0",
    "nuxt-edge": "2.16.0-26984181.2ec62617",
    "nuxt-vite": "^0.0.36",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "release-it": "^14.5.0",
    "siroc": "0.8.0",
    "standard-version": "9.1.1",
    "tailwindcss-dark-mode": "^1.1.7",
    "ts-jest": "^26.5.4"
  }
}
