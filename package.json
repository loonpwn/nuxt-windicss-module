{
  "name": "nuxt-windicss-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:prepare": "pnpm prepare:playground && pnpm stub",
    "stub": "pnpm -r --parallel --filter=./packages/** run stub",
    "build": "pnpm -r --parallel --filter=./packages/** run build",
    "prepare:playground": "cd playground/nuxt3 && pnpm install && pnpm prepare",
    "lint": "eslint \"{packages,test}/**/*.{ts,vue,json,yml,mjs}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxt/test-utils": "3.13.1",
    "@nuxt/types": "^2.18.0",
    "typescript": "5.4.5",
    "@nuxt/eslint-config": "^0.3.13",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/visitor-keys": "^7.14.1",
    "bumpp": "^9.4.1",
    "cheerio": "1.0.0-rc.12",
    "unbuild": "^2.0.0",
    "vite": "^5.3.2",
    "eslint": "9.5.0",
    "eslint-plugin-n": "^17.9.0",
    "vitest": "^1.6.0",
    "windicss": "^3.5.6"
  },
  "version": "2.6.1"
}
